<svg width="200" height="80" viewBox="0 0 200 80" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Main gradient -->
    <linearGradient id="dealGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    
    <!-- Accent gradient -->
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#60a5fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
    
    <!-- Shadow -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="0" dy="2" stdDeviation="3" flood-opacity="0.15"/>
    </filter>
  </defs>
  
  <!-- Main logo container -->
  <g transform="translate(100, 40)" filter="url(#shadow)">
    
    <!-- Background shape -->
    <rect x="-45" y="-18" width="90" height="36" rx="18" fill="url(#dealGradient)" opacity="0.1"/>
    
    <!-- Letter D -->
    <g transform="translate(-30, 0)">
      <path d="M-8 -15 L-8 15 L-2 15 C 6 15 12 9 12 0 C 12 -9 6 -15 -2 -15 Z M-2 -10 C 3 -10 7 -6 7 0 C 7 6 3 10 -2 10 L-3 10 L-3 -10 Z" fill="url(#dealGradient)"/>
    </g>
    
    <!-- Letter E -->
    <g transform="translate(-10, 0)">
      <rect x="-8" y="-15" width="14" height="5" fill="url(#dealGradient)" rx="1"/>
      <rect x="-8" y="-15" width="5" height="30" fill="url(#dealGradient)" rx="1"/>
      <rect x="-8" y="-3" width="12" height="4" fill="url(#dealGradient)" rx="1"/>
      <rect x="-8" y="10" width="14" height="5" fill="url(#dealGradient)" rx="1"/>
    </g>
    
    <!-- Letter A -->
    <g transform="translate(10, 0)">
      <path d="M0 -15 L-8 15 L-4 15 L-2 7 L2 7 L4 15 L8 15 Z M-1 2 L1 2 L0 -5 Z" fill="url(#accentGradient)"/>
      <rect x="-1" y="-12" width="2" height="8" fill="url(#accentGradient)" rx="1"/>
    </g>
    
    <!-- Letter L -->
    <g transform="translate(25, 0)">
      <rect x="-8" y="-15" width="5" height="25" fill="url(#dealGradient)" rx="1"/>
      <rect x="-8" y="10" width="12" height="5" fill="url(#dealGradient)" rx="1"/>
    </g>
    
    <!-- Connecting elements -->
    <g opacity="0.3">
      <circle cx="-30" cy="0" r="2" fill="url(#dealGradient)"/>
      <circle cx="-10" cy="0" r="2" fill="url(#dealGradient)"/>
      <circle cx="10" cy="0" r="2" fill="url(#accentGradient)"/>
      <circle cx="25" cy="0" r="2" fill="url(#dealGradient)"/>
    </g>
    
  </g>
  
  <!-- Tagline -->
  <text x="100" y="68" font-family="system-ui, -apple-system, sans-serif" 
        font-size="8" font-weight="500" fill="#64748b" text-anchor="middle" letter-spacing="1">
    OPERATOR PRICING INTELLIGENCE
  </text>
</svg>